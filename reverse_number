import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int N=sc.nextInt();
        int reversednumber=0;
        while(N!=0){
            int lastdigit=N%10;
            reversednumber=reversednumber*10+lastdigit;
            N=N/10;
        }
        System.out.println(reversednumber);
    }
}

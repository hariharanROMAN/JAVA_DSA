//factorial of the number using recursion
import java.util.*;
public class rec{
    static int fact(int n){
        if(n<=1)
        {
            return 1;
        }
        else
        {
            return n*fact(n-1);
        }
    }
    public static void main(String[] args) {
     Scanner obj=new Scanner(System.in);
     int n=obj.nextInt();
     System.out.println(fact(n));
    }
}

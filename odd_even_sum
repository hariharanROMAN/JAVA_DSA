import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int N=sc.nextInt();
        int M=sc.nextInt();
        int sum=N+M;
        if(sum%2==0)
        {
            System.out.print("even");
        }
        else
        {
            System.out.print("odd");    
        }
    }
}
